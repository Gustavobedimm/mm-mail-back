{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@now/node",
      "config": {
        "includeFiles": [
          "src/templates/**",
          "src/services/**",
          "src/utils/**"
        ]
      }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.js"
      }
    ]
  }
  